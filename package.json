{
  "name": "quesioner-react",
  "version": "1.0.0",
  "description": "crowd-source meetups",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production",
    "start": "serve -s public",
    "heroku-postbuild": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonathanmusila/Quesioner-React.git"
  },
  "author": "Jonathan Musila",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonathanmusila/Quesioner-React/issues"
  },
  "homepage": "https://github.com/jonathanmusila/Quesioner-React#readme",
  "engines": {
    "npm": "6.4.1",
    "node": "10.15.3"
  },
  "dependencies": {
    "coveralls": "^3.0.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-promise-middleware": "^6.1.1",
    "redux-thunk": "^2.3.0",
    "serve": "^11.0.0",
    "yarn": "^1.22.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.19.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^4.3.1",
    "coverage": "^0.3.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "enzyme-to-json": "^3.3.5",
    "express": "^4.17.3",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "react-router-dom": "^5.0.0",
    "reactstrap": "^8.0.0",
    "redux-devtools-extension": "^2.13.8",
    "style-loader": "^0.23.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "jest": {
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
